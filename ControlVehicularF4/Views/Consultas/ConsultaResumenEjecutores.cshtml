                { name: 'Minimo', index: 'Minimo', width: 80, classes: 'warning-njl', align: 'center', sorttype: 'int', summaryType: 'avg' },
                { name: 'Maximo', index: 'Maximo', width: 80, classes: 'warning-njl', align: 'center', sorttype: 'int', summaryType: 'avg' },
